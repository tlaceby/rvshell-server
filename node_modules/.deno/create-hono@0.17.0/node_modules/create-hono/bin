        const convertProjectName = projectName.toLowerCase().replaceAll(/[^a-z0-9\-_]/gm, "-");
        const rewritten = wrangler.replaceAll(PROJECT_NAME, convertProjectName);
        (0, import_node_fs3.writeFileSync)(wranglerPath, rewritten);
      } catch {
      }
    }
  }
);
